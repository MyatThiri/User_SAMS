        <script src="js/owl.carousel.min.js"></script>
        <script src="js/jquery.sticky.js"></script>
        <script src="js/jquery.slicknav.min.js"></script> -->
        <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCqCaX4fFEQRiTJoeclXKZRaFh2fYfps_Y&callback=initMap"></script>
        <!-- <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js"></script> -->
        <!-- <script src="js/jquery.nice-select.js"></script> -->
        <!-- <script src="js/jquery.jCounter-0.1.4.js"></script>

        <script src="js/main.js"></script> -->
        <script src="js/minified/minified.js"></script>
        <script src="js/map-init.js"></script>
    </body>
    <script src="/js/jquery.min.js" charset="utf-8"></script>
    <script type="text/javascript">
      $(function(){
        <% if (messages.warn) { %>
          alert ('<%=messages.warn%>');
        <%
        }
        %>
        <% if (email) {%>
          $('#one').prop("checked", true);
          $('#email').val('<%=email%>');
          $('#password').focus();
        <% } %>
      });
    </script>
</html>
