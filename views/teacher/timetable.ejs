      <div class="left-bar">
          <div id="title">
                <center style="color:white;font-size:45px;font-family:'AR DELANEY';">Student Attendance Management System</center>
          </div>
      </div>
    </div>

        <div class="container" id="menu">
            <div class="navbar-area flex space-between">
                <div class="right-bar flex">
                    <nav>
                        <ul id="mobile" class="main-menu  hidden-xs">
                            <li class="my-active"><a href="/teacher/home">Home</a>
                            </li>

                            <li><a href="#">Teacher</a>
                                <ul class="sub-menu">
                                  <li><a href="/teacher/timetable" >Timetable</a></li>
                                    <li><a href="/teacher/list" >Attendance List</a></li>
                                    <li><a href="/teacher/monthly" >View Monthly Attendance Count</a></li>                                  </ul>
                            </li>
                            <li><a href="#">Password</a>
                                <ul class="sub-menu">
                                    <li><a href="/teacher/change" >Changing Password</a></li>
